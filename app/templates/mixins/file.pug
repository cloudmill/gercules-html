mixin file_card(title)
  .file-card
    +icon("pdf", "svg")
    a(href="#")
      p=title
      span
        +icon("down", "svg")
        | pdf, 15 mb

mixin file_card-2(title)
  .file-card
    +icon("pdf-2", "svg")
    a(href="#")
      p=title
      span
        +icon("down", "svg")
        | pdf, 15 mb

mixin file_list(titles)
  .file-list
    each title in titles
      +file_card(title)

mixin file_list-2(titles)
  .file-list
    each title in titles
      +file_card-2(title)