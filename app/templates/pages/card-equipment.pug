extends ../layouts/default.pug

block variables
  -var pageTitle='Штукатурная станция mono-mix FU (на 220В)'

block content
  .content.card
    .card-top
      .card-sliders
        .card-sliders_thumbs
          .swiper-container
            .swiper-wrapper
              each item in [1, 2, 1, 2, 1]
                .swiper-slide
                  img(src=`../images/equipment-thumb-${item}.png`, alt="")
        .card-sliders_photos
          .swiper-container
            .swiper-wrapper
              - for (let i = 0; i < 5; i++)
                .swiper-slide
                  img(src=`../images/equipment-main.png`, alt="")
      .card-info.content-large-bottom-padding
        +breadcrumbs(['Каталог','Грунтовки',pageTitle])
        .card-top_row
          .card-top_col
            h1 Штукатурная станция
              br
              | mono-mix FU (на 220В)
            .card-info_price
              span 571.600 руб. 
              span (с НДС 20% )
            h2 Преимущества
            ul
              li Небольшой вес и размер штукатурной станции
              li Необходимое напряжение 220В
              li Регулируемая производительность
              li Обслуживаемая шнековая пара (увеличенный ресурс)
                br
                | Возможность использовать как растворонасос
              li Регулярные поставки на склад
          .card-top_col
            .card-rating
              .card-rating_top
                .card-rating_stars
                  - const stars = [1,1,1,1,0]
                  each star in stars
                    if (star)
                      +icon("star-active", "svg")
                    else
                      +icon("star", "svg")
                .card-rating_result 4,2
              .card-rating_parameters
                .card-rating_parameter
                  h3 Качество
                  .card-rating_track
                    .card-rating_progress(style="width: 75%")
                .card-rating_parameter
                  h3 Эффективность
                  .card-rating_track
                    .card-rating_progress(style="width: 75%")
                .card-rating_parameter
                  h3 Внешний вид
                  .card-rating_track
                    .card-rating_progress(style="width: 75%")
                .card-rating_parameter
                  h3 Функциональность
                  .card-rating_track
                    .card-rating_progress(style="width: 75%")
                .card-rating_parameter
                  h3 Экономичность
                  .card-rating_track
                    .card-rating_progress(style="width: 75%")
        .card-links
          .card-links_item.order-link
            a(href="#")
              p
              span Добавить в заказ
          .card-links_item
            a(href="#")
              +icon("question", "svg")
              span Задать вопрос
        a(href="#").card-info_file
          +icon("pdf", "svg")
