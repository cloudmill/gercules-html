                                                                        +icon("rating-red", "svg")

                                        .b-object__tab-btns

                                            button.b-object__tab-btn.b-object__tab-btn--right
                                            button.b-object__tab-btn.b-object__tab-btn--left

                                .tabs__content-item(data-tab="3")

                                    .b-object__tab-row

                                        .b-object__tab-slider

                                            .swiper-container
                                                .swiper-wrapper

                                                    each item in [1, 2, 3, 2]
                                                        .swiper-slide

                                                            .product-card

                                                                .product-card__content

                                                                    .product-card__tags

                                                                        .product-card__tag Хит продаж
                                                                        .product-card__tag.product-card__tag--active Акция
                                                                        .product-card__tag новинка

                                                                    .product-card__image

                                                                        img.product-card__img(src=`images/b-object/product-${item}.jpg`)

                                                                    .product-card__title GE-29 Грунтовка укрепляющая глубокого проникновения, 5, 10 кг

                                                                .product-card__btns

                                                                    .product-card__btn
